<!DOCTYPE html>

<html dir="ltr" lang="en-US">
<head>
	<meta charset="UTF-8" />
	<meta name="robots" content="index, follow" />

	<title><%= @title %></title>
	<meta name="description" content="<%= @description %>" />
	<%= get_canonical_helper %>


	<%= csrf_meta_tags %>

  	<%= @header.html_safe %>



</head>
<body>
	
	<div id="centerDiv">

		<div id="headerDiv"> 
			<div id="headerLeftDiv"></div><!--end headerLeftDiv-->
			<div id="headerRightDiv"><span class="phoneHead">1-801-995-4350</span><br/><a href="/about_us/contact_information" class="topLinks">CONTACT INFO</a> | <%= render "/shared/login_logout" %>                
			</div><!--end headerRightDiv-->
		</div><!--end headerDiv -->

		<div id="navDiv">
			<%= render "shared/zimbi_nav_1" %>
			<%= render "shared/admin_nav" %>
		</div><!--end navDiv-->

		<div id="mainBackDiv">

			<div id="bannerDiv">
					<div id="breadcrumbDiv"><b>YOU ARE HERE : </b><%= "#{request.fullpath}" %></div>
			</div><!--end bannerDiv-->

			<div id="mainDiv">

				<%= @body_1.html_safe %>

			</div><!--end mainDiv-->

			<div id="footerDiv">
				<%= render "shared/zimbi_footer" %>
			</div><!--end footerDiv-->

		</div><!--end mainBackDiv-->

	</div><!--end centerDiv-->

	<%= @end_of_page.html_safe %>
</body>
</html>
